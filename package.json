{
  "name": "gitlab-chrome-extension",
  "version": "0.7.0",
  "description": "Copy Gitlab new branch name to clipboard",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack --progress --colors --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --progress --colors --config webpack.production.config.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "npm run build && npm run changelog && git add -A",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "author": "Leonardo Luiz <leonardokl@hotmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "classnames": "^2.2.5",
    "clipboard": "^1.5.12",
    "conventional-changelog-cli": "^1.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.2",
    "exports-loader": "^0.6.3",
    "imports-loader": "^0.6.5",
    "perfect-scrollbar": "^0.6.12",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.2",
    "whatwg-fetch": "^1.0.0"
  }
}
