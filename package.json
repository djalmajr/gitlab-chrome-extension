{
  "name": "gitlab-chrome-extension",
  "version": "0.1.0",
  "description": "Copy Gitlab new branch name to clipboard",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack --progress --colors --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --progress --colors --config webpack.production.config.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "npm run build && npm run changelog && git add -A",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "author": "Leonardo Luiz <leonardokl@hotmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "clipboard": "^1.5.12",
    "conventional-changelog-cli": "^1.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.2"
  }
}
